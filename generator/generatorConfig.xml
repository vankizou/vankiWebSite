<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <classPathEntry
            location="./mysql-connector-java-5.1.20-bin.jar"/>

    <context id="DB2Tables" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressAllComments" value="false"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>


        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/vanki?generateSimpleParameterMetadata=true"
                        userId="root" password="">
        </jdbcConnection>

        <!-- <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/lofficiel?generateSimpleParameterMetadata=true"
                        userId="root" password="123">
        </jdbcConnection> -->

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- generate Model -->
        <javaModelGenerator targetPackage="com.zoufanqi.entity"
                            targetProject="./src/main/java">
            <property name="enableSubPackages" value="false"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- generate xml -->
        <sqlMapGenerator targetPackage="mapper" targetProject="./src/main/resources">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>

        <!-- generate Mapper -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.zoufanqi.mapper"
                             targetProject="./src/main/java" implementationPackage="">
            <property name="enableSubPackages" value="false"/>
            <property name="exampleMethodVisibility" value="public"/>
        </javaClientGenerator>


        <!--<table tableName="t_env_var" domainObjectName="EnvVar"/>-->
        <!--<table tableName="t_note" domainObjectName="Note"/>-->
        <!--<table tableName="t_note_detail" domainObjectName="NoteDetail"/>-->
        <table tableName="t_picture" domainObjectName="Picture"/>
        <!--<table tableName="t_user" domainObjectName="User"/>-->
        <!--<table tableName="t_auth_role" domainObjectName="AuthRole"/>-->
        <!--<table tableName="t_auth_user_role" domainObjectName="AuthUserRole"/>-->
        <!--<table tableName="t_auth_resource" domainObjectName="AuthResource"/>-->
        <!--<table tableName="t_auth_role_operation" domainObjectName="AuthOperation"/>-->
        <!--<table tableName="t_user_login_record" domainObjectName="UserLoginRecord"/>-->


    </context>
</generatorConfiguration>
